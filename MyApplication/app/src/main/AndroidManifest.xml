<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <application android:icon="@drawable/logo" android:label="@string/app_name">

        <activity android:name=".MainActivity"
            android:label="Campus Cloud"
            android:theme="@style/Theme.AppCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity" android:label="Campus Cloud"
            android:theme="@style/Theme.AppCompat"/>

        <activity android:name=".ActivityUserProfile"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

        <activity android:name=".ActivityParentProfile"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

        <activity android:name=".AppointmentActivity"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

        <activity android:name=".AttendanceActivity"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

        <activity android:name=".ResultActivity"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

        <activity android:name=".StudentActivity"
            android:theme="@style/Theme.AppCompat"
            android:label="Campus Cloud"/>

    </application>
</manifest>